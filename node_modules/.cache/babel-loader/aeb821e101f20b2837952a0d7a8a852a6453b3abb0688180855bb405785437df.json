\n  },\n  data() {\n    return {\n      toggled: false,\n    };\n  },\n  methods: {\n    onTitleClick(evt) {\n      this.$emit(\"title-click\", evt);\n    },\n    closeMenu() {\n      this.toggled = false;\n    },\n  },\n};\n</script>\n<style></style>\n"],"mappings":"AA0CA,OAAOA,kBAAiB,MAAO,sBAAsB;AAErD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,IAAI,EAAE;MACJA,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,SAAS;MAClBC,WAAW,EAAE;IACf,CAAC;IACDC,KAAK,EAAE;MACLJ,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE;IACf,CAAC;IACDE,SAAS,EAAE;MACTL,IAAI,EAAE,CAACC,MAAM,EAAEK,MAAM,CAAC;MACtBJ,OAAO,EAAEK,IAAI,CAACC,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC;MACjCN,WAAW,EACT;IACJ,CAAC;IACDO,MAAM,EAAE;MACNV,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,MAAM;MACfC,WAAW,EAAE;IACf,CAAC;IACDQ,KAAK,EAAE;MACLX,IAAI,EAAEY,OAAO;MACbV,OAAO,EAAE,KAAK;MACdC,WAAW,EAAE;IACf,CAAC;IACDU,WAAW,EAAE;MACXb,IAAI,EAAEY,OAAO;MACbV,OAAO,EAAE,KAAK;MACdC,WAAW,EAAE;IACf,CAAC;IACDW,MAAM,EAAE;MACNd,IAAI,EAAEY,OAAO;MACbV,OAAO,EAAE,KAAK;MACdC,WAAW,EAAE;IACf;EACF,CAAC;EACDY,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,GAAG,EAAE;MAChB,IAAI,CAACC,KAAK,CAAC,aAAa,EAAED,GAAG,CAAC;IAChC,CAAC;IACDE,SAASA,CAAA,EAAG;MACV,IAAI,CAACL,OAAM,GAAI,KAAK;IACtB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}